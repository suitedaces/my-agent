{
  "name": "dorabot",
  "version": "1.0.0",
  "type": "module",
  "description": "OpenClaw-style agent built with Claude Agent SDK",
  "main": "dist/index.js",
  "bin": {
    "dorabot": "dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "(kill $(netstat -anv -p tcp 2>/dev/null | awk '/:18789 .* LISTEN/{print $9}') 2>/dev/null; mkdir -p ~/.dorabot/logs; trap 'kill 0' SIGINT; tsx watch src/index.ts -g 2>&1 | tee ~/.dorabot/logs/gateway.log & npm -C desktop run dev)",
    "dev:gateway": "mkdir -p ~/.dorabot/logs && tsx watch src/index.ts -g 2>&1 | tee ~/.dorabot/logs/gateway.log",
    "dev:cli": "tsx src/index.ts -i",
    "dev:desktop": "npm -C desktop run dev",
    "start": "node dist/index.js -g",
    "typecheck": "tsc --noEmit",
    "test:subagent": "tsx scripts/test-subagent-stream.ts",
    "test:raw-sdk": "tsx scripts/test-raw-sdk-stream.ts"
  },
  "dependencies": {
    "@anthropic-ai/claude-agent-sdk": "^0.1.0",
    "@grammyjs/runner": "^2.0.3",
    "@grammyjs/transformer-throttler": "^1.2.1",
    "@modelcontextprotocol/sdk": "^1.0.0",
    "@openai/codex-sdk": "^0.98.0",
    "@whiskeysockets/baileys": "^6.7.0",
    "better-sqlite3": "^12.6.2",
    "dotenv": "^17.2.4",
    "grammy": "^1.21.0",
    "gray-matter": "^4.0.3",
    "ical-generator": "^10.0.0",
    "mime-types": "^3.0.2",
    "playwright-core": "^1.58.2",
    "qrcode-terminal": "^0.12.0",
    "rrule": "^2.8.1",
    "ws": "^8.18.0",
    "zod": "^3.23.0"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "@types/mime-types": "^3.0.1",
    "@types/node": "^22.0.0",
    "@types/ws": "^8.18.1",
    "tsx": "^4.19.0",
    "typescript": "^5.6.0"
  },
  "engines": {
    "node": ">=22.0.0"
  }
}
